    <style type="text/css">
        .init {
            visibility: hidden;
        }
    </style>
    <script type="text/javascript" src="/public/client/js/custom_init.js"></script>
    <%- block('beforeCommon') %>
    <link rel="stylesheet" type="text/css" href="/public/client/css/common.css"/>
    <link rel="stylesheet" type="text/css" href="/public/client/css/custom.css"/>
    <%- stylesheets %>
    <title>로렌 louren | <%= block('title') %></title>
    <meta name="path_role" content="MAIN"/>
    <meta name="author" content="로렌 louren"/>
    <meta name="description" content="핸드메이드를 기반으로 하는 가구, 리빙 브랜드 로렌"/>
    <meta name="keywords" content="라탄가구, 라탄소품, 라탄의자, 핸드메이드가구"/>
